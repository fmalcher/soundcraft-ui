{
  "version": 1,
  "projects": {
    "mixer-connection": {
      "root": "packages/mixer-connection",
      "sourceRoot": "packages/mixer-connection/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/mixer-connection/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/mixer-connection/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/packages/mixer-connection"
          ]
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/mixer-connection",
            "tsConfig": "packages/mixer-connection/tsconfig.lib.json",
            "packageJson": "packages/mixer-connection/package.json",
            "main": "packages/mixer-connection/src/index.ts",
            "assets": [
              "packages/mixer-connection/*.md"
            ]
          },
          "outputs": [
            "{options.outputPath}"
          ]
        }
      }
    },
    "testbed": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "packages/testbed",
      "sourceRoot": "packages/testbed/src",
      "prefix": "soundcraft-ui",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/packages/testbed",
            "index": "packages/testbed/src/index.html",
            "main": "packages/testbed/src/main.ts",
            "polyfills": "packages/testbed/src/polyfills.ts",
            "tsConfig": "packages/testbed/tsconfig.app.json",
            "aot": true,
            "assets": [
              "packages/testbed/src/favicon.ico",
              "packages/testbed/src/assets"
            ],
            "styles": [
              "packages/testbed/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "packages/testbed/src/environments/environment.ts",
                  "with": "packages/testbed/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "outputs": [
            "{options.outputPath}"
          ]
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "testbed:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "testbed:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "testbed:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "packages/testbed/tsconfig.app.json",
              "packages/testbed/tsconfig.spec.json",
              "packages/testbed/tsconfig.editor.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!packages/testbed/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/testbed/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/packages/testbed"
          ]
        }
      }
    },
    "testbed-e2e": {
      "root": "packages/testbed-e2e",
      "sourceRoot": "packages/testbed-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "packages/testbed-e2e/cypress.json",
            "tsConfig": "packages/testbed-e2e/tsconfig.e2e.json",
            "devServerTarget": "testbed:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "testbed:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "packages/testbed-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!packages/testbed-e2e/**/*"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "testbed",
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  }
}
